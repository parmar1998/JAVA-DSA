package com.cn.m1;

import java.util.ArrayList;
import java.util.List;

public class Solution {
	public static void allFactors(int num) {
		List<Integer> list = new ArrayList<Integer>();
		if (num >= 0 && num <= 10000) {
			for (int i = 1; i <= num; i++) {
				if (num % i == 0) {
					list.add(i);
				}
			}
		}
	}

	public static void main(String[] args) {
	}
}
